{
  "name": "bazooka-city",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "NODE_ENV=production parcel build --no-cache src/client/bundle.ts",
    "watch": "NODE_ENV=development parcel watch --no-cache --no-hmr --no-autoinstall src/client/bundle.ts",
    "test": "npm run lint",
    "lint": "tslint -p . -c tslint.json ./src/**/*.ts ./test/**/*.ts",
    "lint-ts": "tsc --noEmit -p .",
    "lint-css": "stylelint **/*.css --config node_modules/stylelint-config-standard/index.js",
    "start": "NODE_ENV=production parcel build --no-cache --target=node src/server/server.ts && node dist/server"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dcposch/gl.git"
  },
  "license": "UNLICENSED",
  "author": "",
  "bugs": {
    "url": "https://github.com/dcposch/gl/issues"
  },
  "dependencies": {
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.0",
    "@types/gl-matrix": "^2.4.1",
    "@types/murmurhash": "0.0.0",
    "@types/uuid": "^3.4.3",
    "@types/ws": "^6.0.0",
    "compression": "^1.6.2",
    "disc": "^1.3.2",
    "express": "^4.14.0",
    "find-requires": "^0.2.2",
    "fs-extra": "^7.0.0",
    "game-shell": "^1.3.2",
    "gl-mat3": "^2.0.0",
    "gl-mat4": "^1.1.4",
    "gl-quat": "^1.0.0",
    "gl-vec2": "^1.0.0",
    "gl-vec3": "^1.0.3",
    "gl-vec4": "^1.0.1",
    "http-server": "^0.11.1",
    "mkdirp": "^0.5.1",
    "murmurhash": "0.0.2",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-typescript": "^1.0.0",
    "pngjs": "^3.0.0",
    "regl": "^1.2.1",
    "rimraf": "^2.5.4",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "tape": "^4.6.2",
    "uuid": "^3.3.2",
    "walk-sync": "^0.3.1",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.9.1",
    "typescript": "^3.0.1"
  }
}
